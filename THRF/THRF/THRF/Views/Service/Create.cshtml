
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<script type="text/javascript">CKEDITOR.replace('editor')</script>


<div class="form">
    <form class="form-validate form-horizontal" id="feedback_form" method="post" action="/Service/Create/" enctype="multipart/form-data"> 
        <div class="form-group ">
            <label for="cname" class="control-label col-lg-2">Adı-Soyadı <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="SerId" name="SerHead"  type="text" required />
            </div>
        </div>
        <div class="form-group ">
            <label for="cemail" class="control-label col-lg-2">Tc-Kimlik <span class="required">*</span></label>
            <div class="col-lg-10">
                <input class="form-control" id="SerId" name="SerEx"  type="text" required />
            </div>
        </div>
        <div class="form-group ">
            <label for="curl" class="control-label col-lg-2">E-Mail</label>
            <div class="col-lg-10">
                <input class="form-control" id="SerId" name="SerMail"  type="text" required />
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-offset-2 col-lg-10">
                <button class="btn btn-primary" type="submit">Kaydet</button>
                <button class="btn btn-default" type="button"><a href="/Service/Index">İptal</a></button>
            </div>
        </div>
    </form>
</div>

